//- This is the lab view page. It uses lab.js
<!DOCTYPE html>
<html>
<head>
    <title>Lab View - LabProject</title>
    include includes/includes.pug
    <script src="static/js/fabric.js/dist/fabric.js"></script>
    <link rel="stylesheet" type="text/css" href="static/css/lab.css">
</head>
<body>
    
    <main>
        <main-header></main-header>
        <page-notice v-bind:is_visible="notice.is_visible" v-bind:message_contents="notice.contents" v-bind:display_class="notice.class"></page-notice>
        <div id="diagram-viewport">
            <canvas id="diagram-display"></canvas>
            <lab-context-menu :lab="lab" :diagram="diagram" :x="contextmenu.locX" :y="contextmenu.locY" :is-visible="contextmenu.is_visible" v-on:menu-close="contextmenu.on_close"></lab-context-menu>
        </div>
        <status-bar v-bind:lab="lab" v-bind:diagram="diagram"></status-bar>
        <open-lab-modal v-bind:is_open="modal.is_open" v-on:modal_close="close_open_lab_modal"></open-lab-modal>
    </main>

    <script src="static/js/lab.js"></script>
</body>
</html>